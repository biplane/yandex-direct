<?xml version="1.0" ?>
<container xmlns="http://symfony.com/schema/dic/services"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>
        <service id="biplane_yandex_direct.auth" class="Biplane\YandexDirect\Auth\Authenticator" />

        <service id="biplane_yandex_direct.factory" class="Biplane\YandexDirectBundle\UserFactory">
            <argument type="service" id="event_dispatcher" />
        </service>

        <service id="biplane_yandex_direct.ipc_factory" class="Biplane\SysV\Factory" public="false" />

        <service id="biplane_yandex_direct.event_listener.concurrent" class="Biplane\YandexDirect\EventListener\ConcurrentListener">
            <argument type="service" id="biplane_yandex_direct.ipc_factory" />

            <tag name="kernel.event_subscriber" />
        </service>

        <service id="biplane_yandex_direct.dumper" class="Biplane\YandexDirect\Helper\Dumper" />

        <service id="biplane_yandex_direct.event_listener.dump" class="Biplane\YandexDirect\EventListener\DumpListener">
            <argument type="service" id="biplane_yandex_direct.dumper" />

            <tag name="kernel.event_subscriber" />
        </service>
    </services>
</container>
